{
  "name": "zaport",
  "version": "0.0.8",
  "description": "Gracefully closes all processes on a specific port. Target TCP, UDP or both.",
  "license": "MIT",
  "repository": "radiovisual/zaport",
  "author": {
    "name": "Michael Wuergler",
    "email": "wuergler@gmail.com",
    "url": "numetriclabs.com"
  },
  "preferGlobal": true,
  "bin": {
    "zaport": "cli.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "xo && ava --verbose"
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "port",
    "close",
    "all",
    "processes",
    "zap",
    "exit",
    "end",
    "pid",
    "zap",
    "zaport",
    "zapport",
    "kill",
    "process",
    "tcp",
    "udp",
    "socket",
    "network",
    "terminal",
    "node"
  ],
  "dependencies": {
    "array-unique": "^0.2.1",
    "chalk": "^1.1.1",
    "fkill": "^4.0.0",
    "meow": "^3.6.0",
    "multiline": "^1.0.2",
    "native-promise-only": "^0.8.1",
    "port-pid": "0.0.6",
    "selective-whitespace": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.8.0",
    "express": "^4.13.4",
    "noop-server": "0.0.5",
    "xo": "^0.12.1"
  },
  "xo": {
    "ignores": [
      "test/*.js"
    ]
  },
  "ava": {
    "verbose": true
  }
}
